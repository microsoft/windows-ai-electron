{
  "name": "@microsoft/windows-ai-electron",
  "version": "0.2",
  "main": "index.js",
  "types": "index.d.ts",
  "os": [
    "win32"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-windows-ai-electron-arm64": "cd windows-ai-electron && prebuildify --arch arm64 --strip && cd ..",
    "build-windows-ai-electron-x64": "cd windows-ai-electron && prebuildify --arch x64 --strip && cd ..",
    "build-all": "npm run build-windows-ai-electron-arm64 && npm run build-windows-ai-electron-x64",
    "clean": "node -e \"require('fs').rmSync('bin', {recursive: true, force: true})\""
  },
  "keywords": [],
  "license": "MIT",
  "description": "Electron addon which creates JavaScript projections for the native Windows AI APIs.",
  "repository": {
    "type": "git",
    "url": "git@github.com:microsoft/windows-ai-electron.git"
  },
  "devDependencies": {
    "@microsoft/winappcli": "^0.1.7",
    "nan": "^2.23.0",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.4.2",
    "node-gyp-build": "^4.8.4",
    "prebuildify": "^6.0.1"
  },
  "files": [
    "/windows-ai-electron",
    "/LICENSE",
    "/index.js",
    "/index.d.ts"
  ]
}
